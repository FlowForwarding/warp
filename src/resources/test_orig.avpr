     {"name": "OFPL_HELLO_LEN", "default": [0,8]},
     {"name": "OFPL_ERROR_LEN",  "default": [0,0]},
     {"name": "OFPL_ECHO_REQUEST_LEN", "default": [0,0]},
     {"name": "OFPL_ECHO_REPLY_LEN", "default": [0,0]},
