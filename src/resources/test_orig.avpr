    "size": 32,
    "default": {"or": [{"shift": ["OFPXMT_OFB_IN_PORT", 9]},{"shift": ["OFPXMC_OPENFLOW_BASIC", 16]},{"shift": ["OXM_HAS_MASK_UNSET", 8]},"OXM_LENGTH_IN_PORT"]}
